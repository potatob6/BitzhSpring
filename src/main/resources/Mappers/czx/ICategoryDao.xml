<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mappers.czx.ICategoryDao">
    <!--查询新闻类别集合列表 -->
    <select id="selectCategoryList" resultType="ICategory">
        select * from I_category
    </select>
    <!--通过categoryId查询新闻类别 -->
    <select id="getCategoryById" parameterType="Integer" resultType="ICategory">
        select * from I_category where categoryId=#{categoryId}
    </select>
    <select id="getCategoryByname" parameterType="Beans.czx.ICategory">
        select * from ICategory where categoryName=#{categoryName}
    </select>

</mapper>